{
  "name": "@manuscripts/manuscripts-json-schema",
  "version": "1.7.1",
  "description": "",
  "main": "./dist/cjs",
  "module": "./dist/es",
  "types": "./dist/types",
  "scripts": {
    "build": "npm run build-src && npm run build-schemas && npm run build-definitions && npm run build-typelookup && npm run build-validators",
    "build-src": "rimraf tsc-dist/* dist/* schemas/* definitions/* && tsc",
    "build-schemas": "node tsc-dist/exportSchemas",
    "build-definitions": "node tsc-dist/exportDefinitions",
    "build-typelookup": "node tsc-dist/exportManuscriptsTypeLookup",
    "build-validators": "node tsc-dist/exportValidators",
    "prepare": "npm run build",
    "test": "npm run build && tape tests/*.js"
  },
  "publishConfig": {
    "@manuscripts:registry": "https://atypon-npm-registry.herokuapp.com/"
  },
  "sideEffects": false,
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/js-beautify": "0.0.31",
    "@types/node": "^9.6.2",
    "doca": "^0.4.0",
    "json-schema-to-typescript": "^5.4.0",
    "rimraf": "^2.6.2",
    "tape": "^4.8.0",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "ajv": "^6.5.0",
    "js-beautify": "^1.7.5",
    "deepmerge": "^2.0.1"
  }
}
