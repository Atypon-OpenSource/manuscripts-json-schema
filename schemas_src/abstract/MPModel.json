{
  "$id": "MPModel.json",
  "$mash": {
    "sources": [
      { "$ref": "abstract/MPBaseModel.json#" }
    ],
    "with": {
      "properties": {
        "_deleted": {
          "type": "boolean"
        },
        "objectType": {
          "type": "string"
        },
        "sessionID": {
          "type": "string"
        },
        "createdAt": {
          "$ref": "numbers.json#/definitions/timestamp"
        },
        "updatedAt": {
          "$ref": "numbers.json#/definitions/timestamp"
        },
        "keywordIDs" : {
          "type": "array",
          "items": {
            "$ref": "strings.json#/definitions/keywordID"
          }
        }
      },
      "required": [
        "createdAt",
        "updatedAt"
      ]
    }
  }
}
