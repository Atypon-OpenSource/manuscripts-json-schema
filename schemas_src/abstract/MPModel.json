{
  "$id": "MPModel.json",
  "$mash": {
    "sources": [{ "$ref": "abstract/MPBaseModel.json#" }],
    "with": {
      "properties": {
        "createdAt": {
          "$ref": "numbers.json#/definitions/timestamp"
        },
        "updatedAt": {
          "$ref": "numbers.json#/definitions/timestamp"
        },
        "prototype": {
          "$ref": "strings.json#/definitions/_id"
        },
        "keywordIDs": {
          "type": "array",
          "items": {
            "$ref": "strings.json#/definitions/keywordID"
          }
        }
      },
      "required": ["createdAt", "updatedAt"]
    }
  }
}
