{
  "$id": "MPTableElement.json",
  "type": "object",
  "$mash": {
    "sources": [
      { "$ref": "abstract/MPElement.json#" },
      { "$ref": "mixins/MPContainedDocument.json#" },
      { "$ref": "mixins/MPManuscriptDocument.json#" },
      { "$ref": "mixins/MPHighlightable.json#" }
    ],
    "with": {
      "properties": {
        "caption": {
          "type": "string"
        },
        "objectType": {
          "type": "string",
          "enum": ["MPTableElement"]
        },
        "suppressFooter": {
          "type": "boolean"
        },
        "suppressHeader": {
          "type": "boolean"
        },
        "suppressCaption": {
          "type": "boolean"
        },
        "containedObjectID": {
          "$ref": "strings.json#/definitions/_id"
        },
        "tableStyle": {
          "allOf": [
            {
              "$ref": "strings.json#/definitions/_id"
            },
            {
              "type": "string",
              "pattern": "^MPTableStyle:"
            }
          ]
        },
        "paragraphStyle": {
          "allOf": [
            {
              "$ref": "strings.json#/definitions/_id"
            },
            {
              "type": "string",
              "pattern": "^MPParagraphStyle:"
            }
          ]
        },
        "listingID": {
          "allOf": [
            {
              "$ref": "strings.json#/definitions/_id"
            },
            {
              "type": "string",
              "pattern": "^MPListing:"
            }
          ]
        }
      },
      "required": ["containedObjectID"]
    }
  }
}
