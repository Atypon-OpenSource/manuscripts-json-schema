{
  "$id": "MPTableElement.json",
  "type": "object",
  "$mash": {
    "sources": [
      { "$ref": "abstract/MPElement.json#" },
      { "$ref": "mixins/MPContainedDocument.json#" }
    ],
    "with": {
      "properties": {
        "caption": {
          "type": "string"
        },
        "objectType": {
          "type": "string",
          "enum": [ "MPTableElement" ]
        },
        "suppressFooter": {
          "type": "boolean"
        },
        "suppressHeader": {
          "type": "boolean"
        },
        "tableStyle": {
          "allOf": [
            {
              "$ref": "strings.json#/definitions/_id"
            },
            {
              "type": "string",
              "pattern": "^MPTableStyle"
            }
          ]
        },
        "paragraphStyle": {
          "allOf": [
            {
              "$ref": "strings.json#/definitions/_id"
            },
            {
              "type": "string",
              "pattern": "^MPParagraphStyle"
            }
          ]
        }
      },
      "required": []
    }
  }
}
