{
  "$id": "MPManuscriptTemplate.json",
  "title": "ManuscriptTemplate",
  "type": "object",
  "$mash": {
    "sources": [
      { "$ref": "abstract/MPModel.json#" },
      { "$ref": "mixins/MPContainedDocument.json#" },
      { "$ref": "mixins/MPManuscriptDocument.json#" }
    ],
    "with": {
      "properties": {
        "acceptableFigureFormats": {
          "additionalItems": false,
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "acceptableFigureFormatsString": {
          "type": "string"
        },
        "acceptableManuscriptFormats": {
          "additionalItems": false,
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "acceptableTableFormats": {
          "additionalItems": false,
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "aim": {
          "type": "string"
        },
        "authorInstructionsURL": {
          "type": "string"
        },
        "bodyFontSize": {
          "type": "number"
        },
        "bodyLineSpacing": {
          "type": "number"
        },
        "bundle": {
          "$ref": "strings.json#/definitions/_id"
        },
        "category": {
          "$ref": "strings.json#/definitions/_id"
        },
        "conflictOfInterestURL": {
          "type": "string"
        },
        "coverLetterRequirement": {
          "$ref": "strings.json#/definitions/_id"
        },
        "desc": {
          "type": "string"
        },
        "eISSNs": {
          "additionalItems": false,
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "embeddedAuxiliaryElementNumberingStyle": {
          "$ref": "MPNumberingStyle.json#"
        },
        "embeddedFigureElementNumberingStyle": {
          "$ref": "MPNumberingStyle.json#"
        },
        "embeddedLineNumberingStyle": {
          "$ref": "MPNumberingStyle.json#"
        },
        "embeddedPageNumberingStyle": {
          "$ref": "MPNumberingStyle.json#"
        },
        "embeddedTableElementNumberingStyle": {
          "$ref": "MPNumberingStyle.json#"
        },
        "figureElementFileLayout": {
          "type": "number"
        },
        "figureFileLayoutString": {
          "type": "string"
        },
        "figureFileSizeLimit": {
          "type": "string"
        },
        "figureMinResolutionString": {
          "type": "string"
        },
        "hidden": {
          "type": "number"
        },
        "includeLineNumbering": {
          "type": "object",
          "properties": {
            "required": {
              "type": "boolean"
            }
          }
        },
        "includePageNumbering": {
          "type": "object",
          "properties": {
            "inclusive": {
              "additionalItems": false,
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            "required": {
              "type": "boolean"
            }
          }
        },
        "ISSNs": {
          "additionalItems": false,
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "LaTeXTemplateURL": {
          "type": "string"
        },
        "licenses": {
          "additionalItems": false,
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "manuscriptRunningTitleRequirement": {
          "$ref": "strings.json#/definitions/_id"
        },
        "maxCharCountRequirement": {
          "$ref": "strings.json#/definitions/_id"
        },
        "maxCombinedFigureTableCountRequirement": {
          "$ref": "strings.json#/definitions/_id"
        },
        "maxFigureFileSizeRequirement": {
          "$ref": "strings.json#/definitions/_id"
        },
        "maxFigureScreenDPIRequirement": {
          "$ref": "strings.json#/definitions/_id"
        },
        "maxManuscriptTitleCharacterCountRequirement": {
          "$ref": "strings.json#/definitions/_id"
        },
        "maxManuscriptTitleWordCountRequirement": {
          "$ref": "strings.json#/definitions/_id"
        },
        "maxPageCountRequirement": {
          "$ref": "strings.json#/definitions/_id"
        },
        "maxTableCountRequirement": {
          "$ref": "strings.json#/definitions/_id"
        },
        "maxWordCountRequirement": {
          "$ref": "strings.json#/definitions/_id"
        },
        "minCharCountRequirement": {
          "$ref": "strings.json#/definitions/_id"
        },
        "minCombinedFigureTableCountRequirement": {
          "$ref": "strings.json#/definitions/_id"
        },
        "minFigureCountRequirement": {
          "$ref": "strings.json#/definitions/_id"
        },
        "minFigureScreenDPIRequirement": {
          "$ref": "strings.json#/definitions/_id"
        },
        "minPageCountRequirement": {
          "$ref": "strings.json#/definitions/_id"
        },
        "minWordCountRequirement": {
          "$ref": "strings.json#/definitions/_id"
        },
        "notes": {
          "type": "string"
        },
        "objectType": {
          "enum": ["MPManuscriptTemplate"],
          "type": "string"
        },
        "parent": {
          "$ref": "strings.json#/definitions/_id"
        },
        "preferredDialect": {
          "type": "string"
        },
        "priority": {
          "type": "number"
        },
        "publisher": {
          "allOf": [
            {
              "$ref": "strings.json#/definitions/_id"
            },
            {
              "type": "string",
              "pattern": "^MPPublisher:"
            }
          ]
        },
        "requirementIDs": {
          "additionalItems": false,
          "items": {
            "$ref": "strings.json#/definitions/_id"
          },
          "type": "array"
        },
        "requirements": {
          "type": "object"
        },
        "sourceURL": {
          "type": "string"
        },
        "styles": {
          "type": "object"
        },
        "tableElementFileLayout": {
          "type": "number"
        },
        "tableFileLayoutString": {
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "wordTemplateURL": {
          "type": "string"
        }
      },
      "required": ["title"]
    }
  }
}
