{
  "$id": "MPCitationItem.json",
  "title": "CitationItem",
  "$mash": {
    "sources": [{ "$ref": "abstract/MPEmbeddedModel.json#" }],
    "with": {
      "properties": {
        "bibliographyItem": {
          "allOf": [
            {
              "$ref": "strings.json#/definitions/_id"
            },
            {
              "type": "string",
              "pattern": "^MPBibliographyItem"
            }
          ]
        },
        "objectType": {
          "type": "string",
          "enum": ["MPCitationItem"]
        }
      },
      "required": ["bibliographyItem"]
    }
  }
}
