{
    "$id": "MPTitle.json",
    "title": "Title",
    "$mash": {
      "sources": [
        { "$ref": "abstract/MPModel.json#" },
        { "$ref": "mixins/MPContainedDocument.json#" },
        { "$ref": "mixins/MPManuscriptDocument.json#" }
      ],
      "with": {
        "properties": {
          "articleTitle": {
            "type": "string"
          },
          "subtitle": {
            "type": "string"
          },
          "runningTitle": {
            "type": "string"
          },
          "objectType": {
            "type": "string",
            "enum": ["MPTitle"]
          }
        },
        "required": ["articleTitle"]
      }
    }
  }
  