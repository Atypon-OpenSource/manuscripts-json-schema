{
  "$id": "MPEquation.json",
  "title": "Equation",
  "type": "object",
  "$mash": {
    "sources": [
      { "$ref": "abstract/MPModel.json#" },
      { "$ref": "mixins/MPContainedDocument.json#" },
      { "$ref": "mixins/MPManuscriptDocument.json#" }
    ],
    "with": {
      "properties": {
        "objectType": {
          "type": "string",
          "enum": ["MPEquation"]
        },
        "content": {
          "type": "string"
        },
        "format": {
          "type": "string",
          "enum": ["tex-math", "mml:math"]
        }
      },
      "required": ["content", "format"]
    }
  }
}
